<Application x:Class="TictacToeWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TictacToeWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor" Color="DarkOrange"/>        
        <SolidColorBrush x:Key="ButtonColor" Color="Orange"/>        
        <SolidColorBrush x:Key="LineColor" Color="Blue"/>        
    </Application.Resources>
</Application>
